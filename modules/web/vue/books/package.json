{
  "name": "@agrzes/yellow-2020-web-vue-books",
  "version": "1.1.6",
  "description": "",
  "main": "target",
  "types": "types",
  "scripts": {
    "build": "rm -rf target types && tsc",
    "watch": "tsc --watch --preserveWatchOutput",
    "test": "nyc mocha test/**/*.{ts,tsx}"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@agrzes/yellow-2020-common-books": "^1.1.6",
    "@agrzes/yellow-2020-common-data-pouchdb": "^1.1.6",
    "@agrzes/yellow-2020-common-model": "^1.1.6",
    "@agrzes/yellow-2020-web-vue-components": "^1.1.6",
    "@agrzes/yellow-2020-web-vue-router": "^1.1.6"
  },
  "files": [
    "/package.json",
    "/package-lock.json",
    "/LICENSE",
    "/src",
    "/target",
    "/types"
  ],
  "devDependencies": {
    "file-loader": "^6.0.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.4.0"
  }
}
