{
  "name": "@agrzes/yellow-2020-web-action-dashboard",
  "version": "1.1.8",
  "description": "Action dashboard UI.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "true",
    "watch": "webpack -w -d",
    "dev": "webpack serve",
    "deploy": "docker-compose pull && docker-compose up -d"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <grzegorz.abramczyk@tuatara.pl>",
  "license": "MIT",
  "dependencies": {
    "@agrzes/jira-adapter": "^0.0.4",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "axios": "^0.21.1",
    "bootstrap": "^4.6.0",
    "jquery": "^3.5.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "popper.js": "^1.14.6"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.6.0",
    "@types/clipboard": "^2.0.1",
    "@types/jquery": "^3.3.36",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^8.2.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "mocha": "^8.2.1",
    "node-sass": "^5.0.0",
    "nyc": "^15.0.1",
    "sass-loader": "^10.1.1",
    "source-map-loader": "^2.0.0",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "private": true,
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-beta.12",
    "vuex": "^4.0.0-beta.4"
  }
}
