{
  "name": "@agrzes/yellow-2020-web-vue-components",
  "version": "1.1.8",
  "description": "",
  "main": "target",
  "types": "types",
  "scripts": {
    "build": "rm -rf target types && tsc",
    "watch": "tsc --watch --preserveWatchOutput",
    "test": "nyc mocha test/**/*.{ts,tsx}"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@agrzes/yellow-2020-common-ui-model": "^1.1.6",
    "@agrzes/yellow-2020-web-vue-router": "^1.1.6",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "ace-builds": "^1.4.9",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "popper.js": "^1.16.1",
    "vuex": "^3.4.0"
  },
  "files": [
    "/package.json",
    "/package-lock.json",
    "/LICENSE",
    "/src",
    "/target",
    "/types"
  ],
  "devDependencies": {
    "@types/bootstrap": "^4.5.0",
    "@types/jquery": "^3.3.38"
  },
  "peerDependencies": {
    "vue": "^2.6.11",
    "vue-router": "^3.3.4"
  }
}
