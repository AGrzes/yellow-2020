{
  "name": "@agrzes/yellow-2020-web-vue-components",
  "version": "1.1.5",
  "description": "",
  "main": "target",
  "types": "types",
  "scripts": {
    "build": "rm -rf target types && tsc",
    "watch": "tsc -w",
    "test": "nyc mocha test/**/*.{ts,tsx}"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@agrzes/yellow-2020-common-ui-model": "^1.1.5",
    "ace-builds": "^1.4.9",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15"
  },
  "files": [
    "/package.json",
    "/package-lock.json",
    "/LICENSE",
    "/src",
    "/target",
    "/types"
  ]
}
