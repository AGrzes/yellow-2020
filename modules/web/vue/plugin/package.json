{
  "name": "@agrzes/yellow-2020-web-vue-plugin",
  "version": "1.1.8",
  "description": "",
  "main": "target",
  "types": "types",
  "scripts": {
    "build": "rm -rf target types && tsc",
    "watch": "tsc --watch --preserveWatchOutput"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "files": [
    "/package.json",
    "/package-lock.json",
    "/LICENSE",
    "/src",
    "/target",
    "/types"
  ],
  "devDependencies": {},
  "peerDependencies": {
    "vue-router": "^4.0.0-beta.12"
  }
}
