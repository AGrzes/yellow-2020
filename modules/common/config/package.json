{
  "name": "@agrzes/yellow-2020-common-config",
  "version": "1.0.0",
  "description": "",
  "main": "target",
  "types": "types",
  "scripts": {
    "build": "rm -rf target types && tsc",
    "test": "nyc mocha test/**/*.{ts,tsx}"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <abramczykg@gmail.com>",
  "license": "MIT"
}
