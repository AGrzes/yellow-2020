{
  "name": "yellow-2020-web",
  "version": "1.0.0",
  "description": "Web interface of the yellow 2020.",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "test": "nyc mocha test/**/*.{ts,tsx}",
    "watch": "webpack -w -d"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "author": "Grzegorz Abramczyk <grzegorz.abramczyk@tuatara.pl>",
  "license": "MIT",
  "dependencies": {
    "@agrzes/yellow-vue-components": "^1.0.0",
    "@types/pouchdb-adapter-memory": "^6.1.3",
    "axios": "^0.19.2",
    "bootstrap": "^4.1.3",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "popper.js": "^1.14.6",
    "pouchdb-adapter-http": "^7.2.1",
    "pouchdb-core": "^7.2.1",
    "pouchdb-mapreduce": "^7.2.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@types/bootstrap": "^4.1.3",
    "@types/chai-as-promised": "^7.1.2",
    "@types/clipboard": "^2.0.1",
    "@types/jquery": "^3.3.27",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^7.0.2",
    "@types/pouchdb-adapter-http": "^6.1.3",
    "@types/pouchdb-core": "^7.0.5",
    "@types/pouchdb-mapreduce": "^6.1.4",
    "chai-as-promised": "^7.1.1",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "mocha": "^7.1.1",
    "node-sass": "^4.10.0",
    "nyc": "^15.0.0",
    "pouchdb-adapter-memory": "^7.2.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.1.6",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
